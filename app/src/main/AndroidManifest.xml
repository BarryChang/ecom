<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zzy.ecom">

    <application
        android:name=".entities.TransApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainMenu"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".activity.UserSetting"
            android:label="@string/title_activity_user_setting"
            android:parentActivityName=".activity.MainMenu" />
        <activity
            android:name=".activity.AlreadyDoneActivities"
            android:label="@string/title_activity_already_done_activities"
            android:parentActivityName=".activity.MainMenu" />
        <activity
            android:name=".activity.InsideNote"
            android:label="@string/title_activity_inside_note"
            android:parentActivityName=".activity.MainMenu" />
        <activity
            android:name=".activity.InsideActivity"
            android:label="@string/title_activity_inside"
            android:parentActivityName=".activity.MainMenu" />
        <activity
            android:name=".activity.LoginActivity"
            android:label="@string/title_activity_login"
            android:launchMode="singleInstance">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.InsideCommunity"
            android:label="@string/title_activity_inside_community"
            android:parentActivityName=".activity.MainMenu"></activity>
    </application>



</manifest>
